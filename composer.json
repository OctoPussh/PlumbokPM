{
	"name": "octopussh/plumbokpm",
	"type": "library",
	"description": "A PocketMine-MP Plugin that generates code in runtime for your plugins.",
	"license": "OSL-3.0",
	"authors": [
		{
			"name": "Octopush",
			"email": "occtopush@gmail.com"
		}
	],
	"prefer-stable": true,
	"minimum-stability": "dev",
	"autoload": {
		"psr-4": {
			"Octopush\\Plumbok\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Octopush\\Plumbok\\Test\\": "tests/classes/"
		}
	},
	"require": {
		"php": "^8.0.3",
		"doctrine/annotations": "1.*",
		"phpdocumentor/reflection-docblock": "^5.3",
		"symfony/console": "^4.0",
		"nikic/php-parser": "4.*",
		"doctrine/inflector": "^1.3",
      	"ext-yaml": "*"
    },
	"require-dev": {
		"phpstan/phpstan": "^1.2",
		"pocketmine/pocketmine-mp": "^4",
		"mikey179/vfsstream": "^1.6",
		"symfony/var-dumper": "^3.2",
		"phpunit/phpunit": "^9.5",
		"phpunit/php-code-coverage": "^9.2"
	},
	"bin": [
		"./bin/plumbok"
	],
	"config": {
		"bin-dir": "bin/"
	}
}